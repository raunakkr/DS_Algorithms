#include<stdio.h>



int no_divisible(long int n){
	  int c;
	 long int num=n;
	  int count=0;
	while(n>0){


		c=n%10;
		if(c!=0 && num%c==0)
			count++;

		n=n/10;

	}

		return count;

}






